{% load util %}
{
    "sEcho": {{ sEcho|escapejs }},
    "iTotalRecords": {{ iTotalRecords|escapejs }},
    "iTotalDisplayRecords": {{ iTotalDisplayRecords|escapejs }},
    "aaData":[
    {% for vlan in aaData %}
    [
    	"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'><input type='checkbox' name='selection' value='{{ vlan.id|escapejs }}' /></div>",
    	"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'><a href='{% url vlan.edit.by.id vlan.id %}' style='width: 20px; height: 20px;' class='btn_edit'>Editar Vlan</a></div>",
    	"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center table_id' lang='{% url vlan.list.by.id vlan.id %}'>{{ vlan.num_vlan|escapejs }}</div>",
		"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'>{{ vlan.nome|escapejs }}</div>",
		"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'>{{ vlan.ambiente_name|escapejs }}</div>",
		
		"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'>{% ifequal vlan.is_more "True" %}<div class='expandable expandable_{{ vlan.id|escapejs }}'>{% endifequal %}<table style='width: 100%;'>{% for net4 in vlan.redeipv4 %}{% for equip in net4.equipamentos %}<tr>{% if forloop.first %}<th id='expandir_tipo_{{ vlan.id|escapejs }}' lenght='{{ net4.equipamentos|length }}' rowspan='{% if net4.equipamentos|length > 3%} 4 {% else %} {{ net4.equipamentos|length }} {% endif %}'>{{ net4.tipo_rede_name|escapejs }}</th>{% endif %}<td>&nbsp;</td></tr>{% endfor %}{% endfor %}{% for net6 in vlan.redeipv6 %}{% for equip in net6.equipamentos %}<tr>{% if forloop.first %}<th id='expandir_tipo_{{ vlan.id|escapejs }}' lenght='{{ net6.equipamentos|length }}' rowspan='{% if net6.equipamentos|length > 3%} 4 {% else %} {{ net6.equipamentos|length }} {% endif %}'>{{ net6.tipo_rede_name|escapejs }}</th>{% endif %}<td>&nbsp;</td></tr>{% endfor %}{% endfor %}</table></div>{% ifequal vlan.is_more "True" %}<div class='expand' lang='{{ vlan.id|escapejs }}'></div>{% endifequal %}{% ifequal vlan.is_more "True" %}</div>{% endifequal %}",
		
		"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'>{% ifequal vlan.is_more "True" %}<div class='expandable expandable_{{ vlan.id|escapejs }}'>{% endifequal %}<table style='width: 100%;'>{% for net4 in vlan.redeipv4 %}{% for equip in net4.equipamentos %}<tr>{% if forloop.first %}<th id='expandir_rede_{{ vlan.id|escapejs }}' lenght_rede='{{ vlan.redeipv4|length }}' lenght_rede_ipv6='{{ vlan.redeipv6|length }}' lenght='{{ net4.equipamentos|length }}' rowspan='{% if net4.equipamentos|length > 3%} 4 {% else %} {{ net4.equipamentos|length }} {% endif %}'><a href='{% url network.ip4.list.by.id net4.id %}'>{{ net4.network|escapejs }}</a><a href='{% url network.edit.by.id.rede.ipv4 net4.id %}' style='width: 12px; height: 12px;' class='btn_edit'>Editar Rede</a></th>{% endif %}<td>&nbsp;</td></tr>{% endfor %}{% endfor %}{% for net6 in vlan.redeipv6 %}{% for equip in net6.equipamentos %}<tr>{% if forloop.first %}<th id='expandir_rede_{{ vlan.id|escapejs }}' lenght='{{ net6.equipamentos|length }}' rowspan='{% if net6.equipamentos|length > 3 %} 4 {% else %} {{ net6.equipamentos|length }} {% endif %}''><a href='{% url network.ip6.list.by.id net6.id %}'>{{ net6.network|escapejs }}</a><a href='{% url network.edit.by.id.rede.ipv6 net6.id %}' style='width: 12px; height: 12px;' class='btn_edit'>Editar Rede</a></th>{% endif %}<td>&nbsp;</td></tr>{% endfor %}{% endfor %}</table></div>{% ifequal vlan.is_more "True" %}<div {% ifequal vlan.more_than_three "True" %} style='background: url(/media/img/max_red.png) transparent bottom center no-repeat' {% endifequal %} class='expand' id='rede_{{ vlan.id|escapejs }}' lang='{{ vlan.id|escapejs }}'></div>{% endifequal %}{% ifequal vlan.is_more "True" %}</div>{% endifequal %}",
		
		"<div class='{% if vlan.acl_file_name == None and vlan.acl_file_name_v6 == None %}text_red {% endif %}text_center'>{% ifequal vlan.is_more "True" %}<div class='expandable expandable_{{ vlan.id|escapejs }}'>{% endifequal %}<table style='width: 100%;'>{% for net4 in vlan.redeipv4 %}{% for equip in net4.equipamentos %}<tr><td>{{ equip|escapejs }}</td></tr>{% endfor %}{% endfor %}{% for net6 in vlan.redeipv6 %}{% for equip in net6.equipamentos %}<tr><td>{{ equip|escapejs }}</td></tr>{% endfor %}{% endfor %}</table></div>{% ifequal vlan.is_more "True" %}<div class='expand' lang='{{ vlan.id|escapejs }}'></div>{% endifequal %}{% ifequal vlan.is_more "True" %}</div>{% endifequal %}",
		
		
		"<div class='{% if vlan.acl_valida == "False" %}text_red {% endif %}text_center'>{% ifnotequal vlan.acl_file_name None %}{% if vlan.acl_file_name|length > 32 %}<label title='{{ vlan.acl_file_name|escapejs }}'>{% endif %}{{ vlan.acl_file_name|substr|escapejs }}{% if vlan.acl_file_name.length > 32 %}</label>{% endif %}{% endifnotequal %}</div>",
		"<div class='{% if vlan.acl_valida_v6 == "False" %}text_red {% endif %}text_center'>{% ifnotequal vlan.acl_file_name_v6 None %}{% if vlan.acl_file_name_v6|length > 32 %}<label title='{{ vlan.acl_file_name_v6|escapejs }}'>{% endif %}{{ vlan.acl_file_name_v6|substr|escapejs }}{% if vlan.acl_file_name_v6.length > 32 %}</label>{% endif %}{% endifnotequal %}</div>"
	]
    {% if not forloop.last %}
    ,
    {% endif %}
    {% endfor %}
    ]
}
